{
  "name": "mplsart-apiv4",
  "version": "1.0.0",
  "description": "Node/Express API for MPLSART",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "test": "jest",
    "check-types": "tsc --noemit",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "eslint": "eslint . --ext .ts",
    "lint": "npm run eslint && npm run check-types",
    "deploy": "gcloud --verbosity=debug --project arts-612 app deploy app.yaml --no-promote --quiet --version",
    "bd": "npm run lint && npm run build && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mplsart/mplsart-apiv6.git"
  },
  "keywords": [],
  "author": "Blaine Garrett <blaine@mplsart.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mplsart/mplsart-apiv6/issues"
  },
  "homepage": "https://github.com/mplsart/mplsart-apiv6#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.29",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@google-cloud/datastore": "^6.6.2",
    "@supabase/supabase-js": "^1.35.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "firebase-admin": "^10.2.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^3.14.1",
    "typescript-optional": "^3.0.0-alpha.3",
    "zod": "^3.14.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "GOOGLE_APPLICATION_CREDENTIALS=./local/local-dev-service-account.json ts-node src/index.ts"
  }
}
